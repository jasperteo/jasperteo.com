{
	"$schema": "node_modules/wrangler/config-schema.json",

	"name": "jasperteo",

	"compatibility_date": "2025-09-15",
	"compatibility_flags": ["nodejs_compat"],

	"main": "./src/server.ts",

	"routes": [
		{ "custom_domain": true, "pattern": "jasperteo.com" },
		{ "custom_domain": true, "pattern": "www.jasperteo.com" },
	],
	"workers_dev": false,

	"preview_urls": false,

	"observability": { "enabled": true },

	"vars": {
		"BUN_VERSION": "1.3.0",
		"NODE_VERSION": "25.0.0",
	},
}
