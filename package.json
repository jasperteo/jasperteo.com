{
	"name": "jasperteo.com",
	"private": true,
	"version": "2.0.0",
	"type": "module",
	"engines": {
		"node": ">=24 <25",
		"pnpm": ">=10.17"
	},
	"devEngines": {
		"runtime": {
			"name": "node",
			"version": "^24.8.0",
			"onFail": "download"
		}
	},
	"packageManager": "pnpm@10.17.0",
	"scripts": {
		"dev": "vite",
