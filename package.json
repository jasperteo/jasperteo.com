		"next-themes": "^0.4.6",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"rough-notation": "^0.5.1",
		"tailwind-merge": "^3.3.1"
	},
	"devDependencies": {
		"@eslint-react/eslint-plugin": "^1.53.1",
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.36.0",
		"@prettier/plugin-oxc": "^0.0.4",
