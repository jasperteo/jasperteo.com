		"eslint-plugin-oxlint": "^1.16.0",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-unicorn": "^61.0.2",
		"fontless": "^0.0.2",
		"globals": "^16.4.0",
